<p id="notice"><%= notice %></p>

<h1>Interview Notes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Application</th>
      <th>Contact</th>
      <th>Date</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interview_notes.each do |interview_note| %>
      <tr>
        <td><%= interview_note.User %></td>
        <td><%= interview_note.Application %></td>
        <td><%= interview_note.Contact_id %></td>
        <td><%= interview_note.date %></td>
        <td><%= interview_note.notes %></td>
        <td><%= link_to 'Show', interview_note %></td>
        <td><%= link_to 'Edit', edit_interview_note_path(interview_note) %></td>
        <td><%= link_to 'Destroy', interview_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interview Note', new_interview_note_path %>
